  "license": "GPL-3.0-only",
  "main": "src/index.js",
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "dependencies": {
    "@kentcdodds/react-workshop-app": "^2.11.2",
    "@testing-library/react": "^10.3.0",
    "chalk": "^4.1.0",
    "codegen.macro": "^4.0.0",
    "react": "0.14.0",
    "react-dom": "0.0.0-experimental-33c3af284",
    "react-error-boundary": "^2.2.2",
    "react-icons": "^3.10.0"
  },
  "devDependencies": {
    "husky": "^4.2.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "react-scripts": "^3.4.1",
    "serve": "^11.3.2"
  },
  "scripts": {
    "serve": "serve -s build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:profile": "react-scripts build --profile",
    "test": "react-scripts test --env=jsdom",
    "test:coverage": "npm run test -- --watchAll=false",
    "test:exercises": "npm run test -- testing.*exercises\\/ --onlyChanged",
    "setup": "node ./scripts/setup",
    "lint": "eslint .",
    "validate": "npm-run-all --parallel build test:coverage lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run validate"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/final/**/*.js"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "babel": {
    "presets": [
      "babel-preset-react-app"
    ]
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ]
  }
